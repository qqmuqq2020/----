package hr.manage.entity;

import java.util.Date;

public class Time {
	//时间主键id
    private Integer timeid;
    //上班时间
    private String ontime;
    //下班时间
    private String undertime;
    private String afternoontimetop;
    private String afternoontimebelow;

    public Integer getTimeid() {
        return timeid;
    }

    public void setTimeid(Integer timeid) {
        this.timeid = timeid;
    }

   

	public Time() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Time(String ontime, String undertime) {
		super();
		this.ontime = ontime;
		this.undertime = undertime;
	}

	public String getOntime() {
		return ontime;
	}

	public void setOntime(String ontime) {
		this.ontime = ontime;
	}

	public String getUndertime() {
		return undertime;
	}

	public void setUndertime(String undertime) {
		this.undertime = undertime;
	}

	public String getAfternoontimetop() {
		return afternoontimetop;
	}

	public void setAfternoontimetop(String afternoontimetop) {
		this.afternoontimetop = afternoontimetop;
	}

	public String getAfternoontimebelow() {
		return afternoontimebelow;
	}

	public void setAfternoontimebelow(String afternoontimebelow) {
		this.afternoontimebelow = afternoontimebelow;
	}

	
}