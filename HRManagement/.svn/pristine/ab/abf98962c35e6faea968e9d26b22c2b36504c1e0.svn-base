.providerAdd {
	
    padding: 5px;
    margin: 5px;
}
/*树状菜单*/
.tree {
	padding: 0 0 0 30px;
	width: 200px;
}  
.tree li {
	position: relative;
	margin-left: -15px;
} 

.tree li.file{
	margin-left: -18px !important;
}  
.tree li.file a{
	background: url(document.png) 0 0 no-repeat;color: #002446;
	padding-left: 21px;text-decoration:none;display: block;
}  
.tree li input{
	position: absolute;
	left: 0;margin-left: 0;
	opacity: 0;z-index: 2;
	cursor: pointer;height: 1em;
	width:1em;top: 0;
}  
.tree input + ol{
	display: none;
}  
.tree input + ol > li {
	height: 0; overflow: hidden;
	 margin-left: -14px !important;
	  padding-left: 1px; 
	  }  
.tree li label {
	cursor: pointer;display: block;
	padding-left: 0px;
	background: url(toggle-small-expand.png) no-repeat 0px 1px;
	}  
.tree input:checked + ol {
	background: url(toggle-small.png) 44px 5px no-repeat;
	margin: -22px 0 0 -44px;
	padding:27px 0 0 80px;
	height: auto;display: block;
	}  
.tree input:checked + ol > li { 
	height: auto;
	}


.providerAdd .tree{
	width:15%;
	float:left;
}

.providerAdd .right{
	width:75%;
	float:left;
	height:100%;
	padding-top:30px;
}

.providerAdd label {
